{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["airbnb-base", "airbnb-base", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "globals": {
    "var1": "readonly",
    "var2": "readonly",
    "module": true
  },
  "plugins": ["import", "prettier"],
  "rules": {
    "no-console": "warn",
    "quotes": ["error", "double", { "avoidEscape": true }],
    "no-return-assign": "error",
    "no-shadow": "off",
    "import/named": 0,
    "no-useless-escape": 0,
    "no-underscore-dangle": 0,
    "no-throw-literal": 0,
    "global-require": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-useless-catch": 0,
    "no-tabs": 0,
    "consistent-return": 0,
    "new-cap": 0,
    "no-undef": 0,
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ],
    "no-unused-expressions": "off"
  },
  "parser": "@babel/eslint-parser"
}
